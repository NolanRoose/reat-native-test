{
  "expo": {
    "name": "my-car-spot-app",
    "slug": "my-car-spot-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/icon.png",
    "splash": {
      "image": "./public/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.mycarspot.app"
    },
    "web": {
      "favicon": "./public/favicon.png"
    },
    "android": {
      "package": "io.mycarspot.app"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
